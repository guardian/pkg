{
	"extends": "@guardian/eslint-config",
	"ignorePatterns": ["dist", "script"],
	"overrides": [
		{
			"files": ["*.ts"],
			"extends": "@guardian/eslint-config-typescript",
			"rules": {
				"@typescript-eslint/no-var-requires": "off",
				"@typescript-eslint/explicit-module-boundary-types": "off"
			}
		}
	]
}
